# Variables

x = 100

name = "Sameer"


print('name')
print(name) #Variables to be used without quotes
print(type(name)) #str


print(x) #Variables to be used without quotes
print(type(x)) #int

# allowed variable names
'''
iam
i_am
i_am_
iam_
_iam
_iam_
i_am_no1
i_am_no1
'''

# not allowed
'''
@coolb#y
$$123Maddy
%%123boy%%
123
'''

# allowed but not recommended
# double underscore dunder variable

'''
__javed__
__rahul__
__class__

'''
